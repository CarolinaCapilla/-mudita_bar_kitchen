<div class="bg-section-kitchen page-hero-section menu-hero">
  <div class="wrapper w-container">
    <h1 class="page-hero-header">The Menu</h1>
  </div>
</div>

<div class="section pattern-section">
  <div class="wrapper w-container">
    <div class="kitchen-cards"></div>
    <div class="kitchen-cards w-dyn-list">
      <div role="list" class="w-dyn-items">
        <div role="listitem" class="w-dyn-item">
          <% @kitchens.each do |kitchen| %>
          <div class="kitchen-card">
            <div class="kitchen-card-info">
              <div class="kitchen-card-content"><a href="" class="badge kitchen-card-category"></a>
                <a href="" class="kitchen-card-header-link w-inline-block">
                <h3><%= kitchen.name %></h3>
                </a>
                <div class="divider"></div>
                <p class="grey-text">fdsfdsfdsfds</p>
                <div class="kitchen-card-date"><%= kitchen.cuisine %></div>
                <div>
                <%= link_to "Explore Kitchen", kitchen_path(kitchen), class: "main-button" %>
                </div>
              </div>
              <div class="frame"></div>
            </div>
            <% if kitchen.photo.attached? %>
            <div class="kitchen-card-image w-inline-block">
            <%= cl_image_tag kitchen.photo.key %>
            </div>
            <% end %>
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>


