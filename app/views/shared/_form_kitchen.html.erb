<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <%= simple_form_for @kitchen do |f| %>
        <%= f.input :name %>
        <%= f.input :cuisine %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
