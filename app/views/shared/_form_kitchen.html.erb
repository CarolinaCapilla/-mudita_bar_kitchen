<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <%= simple_form_for @kitchen do |f| %>
        <%= f.input :name %>
        <%= f.input :cuisine, collection: Kitchen::CUISINE %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
